    getUserDetails();
  });

  function getUserDetails() {
    if ($('#customer_users').length > 0) {
      var user_id = $('#customer_users').val();

      $.ajax({
        url: 'index.php?route=customer/customer/getUserDetails&user_token={{ user_token }}',
        type: 'POST',
        dataType: 'json',
        data: { 'user_id' : user_id },
        cache: false,
        success: function(json) {
          console.log(json);
          if(json) {
            if(json.title != '') { $('#title').val(json.title); $('#title').prop('disabled', true); }
            if(json.firstname != '') { $('#first_name').val(json.firstname); $('#first_name').prop('disabled', true); }
            if(json.lastname != '') { $('#last_name').val(json.lastname); $('#last_name').prop('disabled', true); }
            if(json.telephone != '') { $('#user_phone').val(json.telephone); }
            if(json.ext != '') { $('#user_ext').val(json.ext); }
            if(json.telephone_1 != '') { $('#user_res_phone').val(json.telephone_1); }
            if(json.telephone_2 != '') { $('#user_cell_phone').val(json.telephone_2); }
            if(json.job_title != '') { $('#job_title').val(json.job_title); }
            if(json.email != '') { $('#user_email').val(json.email); }
          }
        } 
      });
    }
  }

